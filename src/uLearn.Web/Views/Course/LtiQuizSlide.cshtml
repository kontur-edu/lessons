@model LtiQuizModel

@{
	Layout = null;
}

<!DOCTYPE html>
<html>
<head>
	<title>Quiz</title>
	@Styles.Render("~/css")
</head>
<body class="lti-component">
	@if (Model != null)
	{
		@Html.Action("Quiz", "Quiz", new { slide = Model.Slide, courseId = Model.CourseId, userId = Model.UserId, isGuest = false, isLti = true });
		@Scripts.Render("~/main-js")
	}
</body>
</html>