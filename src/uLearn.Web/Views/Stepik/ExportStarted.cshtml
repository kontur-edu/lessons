@model Ulearn.Core.Courses.Course

@{
	ViewBag.Title = "Копирование курса началось";
}

<div class="container">
	<h2>Копирование курса «@Model.Title» началось</h2>
	<p class="help-block">
		Мы пришлём уведомление после окончания копирования.
	</p>
	@Html.RouteLink("Вернуться в курс", "Course.Slide", new { courseId = Model.Id }, new { @class = "btn btn-large btn-success "})
</div>
