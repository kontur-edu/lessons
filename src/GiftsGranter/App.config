<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net" />
  </configSections>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7" />
  </startup>
  <connectionStrings>
    <add name="DefaultConnection" connectionString="Data Source=K1707004;Initial Catalog=ulearn;Integrated Security=True" providerName="System.Data.SqlClient" />
  </connectionStrings>
  <appSettings>

    <!-- Данные для интеграции с Телеграмом -->
    <!-- Токен бота. Получать у t.me/BotFather -->
    <add key="ulearn.telegram.botToken" value="293737221:AAGaAxFmEUjmiyixyy6VNPsaqc_G5ChSYOg" />
    <!-- Канал в телеграме, куда отправлять информацию об ошибках -->
    <add key="ulearn.telegram.gifts.channel" value="@ulearngifts" />
    <add key="ulearn.telegram.errors.channel" value="" />
  </appSettings>   
  <log4net>
    <root>
      <level value="DEBUG" />
      <appender-ref ref="FileAppender" />
      <appender-ref ref="ConsoleAppender" />
      <appender-ref ref="TelegramAppender" />
    </root>
    <appender name="FileAppender" type="log4net.Appender.FileAppender">
      <file value="logs\\gifts.log" />
      <appendToFile value="true" />
      <encoding value="utf-8" />
      <layout type="log4net.Layout.PatternLayout">
        <param name="ConversionPattern" value="%date [%thread] %-5level %logger — %message%newline" />
      </layout>
    </appender>
    <appender name="ConsoleAppender" type="log4net.Appender.ColoredConsoleAppender">
      <mapping>
        <level value="ERROR" />
        <foreColor value="Red, HighIntensity" />
      </mapping>
      <mapping>
        <level value="DEBUG" />
        <foreColor value="Green" />
      </mapping>
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date %-5level %logger — %message%newline" />
      </layout>
    </appender>

    <!--
     Для работы этого аппендера должны быть установлены переменные в appsettings.json:
       telegram:botToken
       telegram:errors:channel 
    -->
    <appender name="TelegramAppender" type="Ulearn.Core.Logging.TelegramAppender, Core">
      <threshold value="Error" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date [%thread] %-5level %logger — %message%newline" />
      </layout>
    </appender>  

  </log4net>


